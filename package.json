{
  "name": "multi-platform-stock-tracker",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "clean": "rm -r dist",
    "postinstall": "echo 'installing watchman locally...' && scripts/install-watchman.sh",
    "watchman": "lib/bin/watchman $@",
    "watchman-make": "lib/bin/watchman-make $@",
    "watchman-build": "lib/bin/watchman-build $@",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "web": "BABEL_ENV='dev-web' webpack --config build-config/webpack.web.config.js --mode development && URL=dist/index.html scripts/launch-web.sh",
    "desktop": "BABEL_ENV='dev-web' webpack --config build-config/webpack.desktop.config.js --mode development && electron ./dist/bundle.desktop.js",
    "react-native": "node node_modules/react-native/local-cli/cli.js $@",
    "cmd": "node_modules/.bin/$@",
    "test": "jest --config test/jest.config.json"
  },
  "dependencies": {
    "copy-webpack-plugin": "^4.5.4",
    "electron": "^3.0.5",
    "react": "16.6.0-alpha.8af6728",
    "react-art": "16.6.0-alpha.8af6728",
    "react-dom": "16.6.0-alpha.8af6728",
    "react-native": "0.57.3",
    "react-native-electron": "^0.9.0",
    "react-native-web": "^0.9.3"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@types/node": "^10.12.2",
    "babel-jest": "23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "core-js": "^2.5.7",
    "html-webpack-plugin": "^3.2.0",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.48.1",
    "react-test-renderer": "16.6.0-alpha.8af6728",
    "ts-loader": "^5.3.0",
    "typescript": "^3.1.6",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2"
  }
}
