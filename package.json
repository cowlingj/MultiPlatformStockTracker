{
  "name": "multi-platform-stock-tracker",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "clean": "rm -r dist",
    "android": "node node_modules/react-native/local-cli/cli.js run-android --config $(pwd)/build-config/rn-cli.config.js",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios --config $(pwd)/build-config/rn-cli.config.js",
    "web": "webpack --config build-config/webpack.web.config.js --mode development && URL=dist/index.html scripts/launch-web.sh",
    "desktop": "webpack --config build-config/webpack.desktop.config.js --mode development && electron ./dist/bundle.desktop.js",
    "react-native": "node node_modules/react-native/local-cli/cli.js $@",
    "cmd": "node_modules/.bin/$@",
    "test-web": "BABEL_ENV='dev-web' jest --config test/jest.config.json",
    "pretty": "prettier --write --config 'prettierrc.json' --ignore-path '.prettierignore' * {src,test,build-config}/**",
    "lint": "tslint --project './build-config/tsconfig.json' --config './.tslint.json' *.{js,ts,jsx,tsx} {src,test,build-config}/**/*.{js,ts,jsx,tsx}"
  },
  "dependencies": {
    "electron": "^3.0.5",
    "react": "16.6.0-alpha.8af6728",
    "react-art": "16.6.0-alpha.8af6728",
    "react-dom": "16.6.0-alpha.8af6728",
    "react-native": "^0.57.4",
    "react-native-electron": "^0.9.0",
    "react-native-web": "^0.9.3"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@types/node": "^10.12.2",
    "@types/react": "^16.4.18",
    "@types/react-native": "^0.57.7",
    "babel-jest": "23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "clean-webpack-plugin": "^0.1.19",
    "core-js": "^2.5.7",
    "html-webpack-plugin": "^3.2.0",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.48.1",
    "prettier": "^1.15.2",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.6.0-alpha.8af6728",
    "ts-loader": "^5.3.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.6",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2"
  }
}
